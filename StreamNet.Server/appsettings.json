{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=StreamNetMediaDatabase;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileStoreOptions": {
    "VideoPath": "%UserProfile%\\StreamNet\\MediaLibrary\\Videos",
    "AudioPath": "%UserProfile%\\StreamNet\\MediaLibrary\\Music",
    "DumpPath": "%UserProfile%\\StreamNet\\NewMedia",
    "RejectPath": "%UserProfile%\\StreamNet\\Rejected",
    "ProfileContent": "%UserProfile%\\StreamNet\\Profiles"
  }
}
